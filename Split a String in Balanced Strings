class Solution:
    def balancedStringSplit(self, s: str) -> int:    
        count=0
        result= 0
        for letter in  s:
            if (letter == 'R'):
                count+=1
            if (letter  == 'L'):
                 count-=1        
            if(count == 0):
                result+=1        
        return result
    
        
        
        
        
